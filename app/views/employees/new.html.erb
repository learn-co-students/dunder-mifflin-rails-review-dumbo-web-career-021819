<h1>New Employee</h1>
<p></p>

<%= form_for @employee do |f|%>
<%= f.label :first_name %>:
<%= f.text_field :first_name%>
<p></p>
<%= f.label :last_name %>:
<%= f.text_field :last_name%>
<p></p>
<%= f.label :alias %>:
<%= f.text_field :alias%>
<p></p>
<%= f.label :title %>:
<%= f.text_field :title%>
<p></p>
<%= f.label :office %>:
<%= f.text_field :office%>
<p></p>
<%= f.label :img_url %>:
<%= f.text_field :img_url%>
<p></p>
<%= f.label :dog_id %>:
<p></p>
<%= f.collection_select :dog_id, @dogs, :id, :name %>
<p></p>
<%= f.submit %>
<% end %>

